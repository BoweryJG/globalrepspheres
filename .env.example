# Copy this file to .env and fill in your values

# Google Analytics 4 Measurement ID
# Get this from your Google Analytics property settings
REACT_APP_GA_ID=G-XXXXXXXXXX

# Chatbot API Keys (required for chatbot functionality)
# Get OpenRouter API key from https://openrouter.ai/
REACT_APP_OPENROUTER_API_KEY=your_openrouter_api_key_here

# Get Brave Search API key from https://brave.com/search/api/
REACT_APP_BRAVE_API_KEY=your_brave_api_key_here

# Add other environment variables below as needed
# REACT_APP_API_URL=https://api.example.com
# REACT_APP_SUPABASE_URL=
# REACT_APP_SUPABASE_ANON_KEY=

# Backend API URL
REACT_APP_BACKEND_URL=http://localhost:5000